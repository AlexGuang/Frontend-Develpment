
<%- include("headering") -%> 
<body>
    <div id="heading" class="box">
        <h1>
            <%= pageTitle %>
        </h1>
    </div>

    <div class="box">
        <% for (var i = 0; i < toDoItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= toDoItems[i] %>
                </p>
            </div>
            <% } %>

                <form action="/" method="post" class="item">
                    <input type="text" placeholder="请输入新的计划" autocomplete="off" name="toDoItem">
                    <button type="submit" name="submitItem" value= <%= pageTitle %> >+</button>
                </form>
    </div>
<%- include("footering")  -%>
